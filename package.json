{
  "name": "api-test-cypress",
  "version": "0.0.24",
  "description": "AAISDirect API TESTING",
  "homepage": "https://aaisonline.atlassian.net/wiki/spaces/MSSI/overview",
  "bugs": {
    "email": "softwaredevelopment@aaisonline.com"
  },
  "scripts": {
    "parallel": "CI=1 npx cypress run --record --key d76932dd-1ca1-4d55-99cf-f11331f2187a --parallel --ci-build-id circle",
    "cypress:run": "cypress run -- headless --record --key d76932dd-1ca1-4d55-99cf-f11331f2187a",
    "test-watch": "jest --coverage --silent --verbose --detectOpenHandles --watchAll",
    "prune": "npm prune --production",
    "manifest": "cypress run -b chrome --headless --spec 'cypress/e2e/api-testing/manifest/manifest.feature'",
    "historical": "cypress run -b chrome --headless --spec 'cypress/e2e/api-testing/browser/historicalDate.feature'",
    "login": "cypress run -b chrome --headless --spec 'cypress/e2e/api-testing/authorization/login.feature'",
    "cy:test": "npm run e2e-test || npm run e2e-test-report",
    "e2e-test": "CI=1 cypress run -b chrome --headless --record --key d76932dd-1ca1-4d55-99cf-f11331f2187a",
    "delete:reports": "rimraf cypress/reports/* || true",
    "pree2e-test": "npm run delete:reports",
    "pree2e-test-report": "npx mochawesome-merge ./cypress/reports/mochawesome/mochawesome*.json -o cypress/reports/mochawesome/merged-mochawesome.json",
    "e2e-test-report": "npm run pree2e-test-report && npx mochawesome-report-generator --cdn false --reportDir cypress/reports --reportPageTitle Project_Jamaica --inline true --reportFilename CypressTestResults.html --reportTitle PJ_version_$npm_package_version --charts true cypress/reports/mochawesome/merged-mochawesome.json",
    "bulletins": "cypress run -b chrome --headless --spec 'cypress/e2e/api-testing/bulletins/bulletins.feature'",
    "hubspot": "cypress run -b chrome --headless --spec 'cypress/e2e/api-testing/hubspot/hubspot.feature'",
    "cypress:runDefault": "cypress run --spec cypress/e2e/api-testing/**/**",
    "cypress:chrome": "cypress run --browser chrome --spec cypress/e2e/api-testing/**/**",
    "cypress:edge": "cypress run --browser edge --spec cypress/e2e/api-testing/**/**",
    "start": "http-server app -c-1",
    "start:ci": "http-server app -c-1 --silent",
    "dev": "npm start -- -o",
    "release": "releaser",
    "types": "tsc --noEmit",
    "test": "npm start & cypress run",
    "pretest": "npm run lint && npm run types",
    "print-env": "print-env",
    "lint": "eslint --fix cypress/**/*.js app/assets/js/scripts.js",
    "colon:names": "colon-names",
    "e2e": "cypress run",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:record": "cypress run --record",
    "e2e:record:parallel": "cypress run --record --parallel",
    "test:ci": "run-p --race start:ci e2e",
    "test:ci:chrome": "run-p --race start:ci e2e:chrome",
    "test:ci:record": "run-p --race start:ci e2e:record",
    "test:ci:record:parallel": "run-p --race start:ci e2e:record:parallel",
    "cy:verify": "cypress verify",
    "cy:version": "cypress version",
    "cy:run": "cypress run",
    "cy:run:record": "cypress run --record",
    "cy:open": "cypress open",
    "local:open": "start-test 8080 cy:open",
    "local:run": "start-test 8080 cy:run",
    "local:run:record": "start-test 8080 cy:run:record",
    "stop-only": "stop-only -f cypress/integration",
    "warn-only": "stop-only -f cypress/integration --warn",
    "ci:set-port": "node ./scripts/set-port"
  },
  "repository": {
    "type": "git",
    "url": "http://build.dev.aaisdirect.com:3000/member-self-service/rest-api.git"
  },
  "keywords": [
    "API",
    "Direct",
    "AAIS",
    "TESTING"
  ],
  "author": {
    "name": "AAIS Development Team",
    "email": "softwaredevelopment@aaisonline.com",
    "url": "https://aaisonline.atlassian.net/wiki/spaces/DEV/overview"
  },
  "contributers": [
    {
      "name": "Andrei Selvanovich",
      "email": "andreis@aaisonline.com",
      "url": "https://aaisonline.atlassian.net/wiki/spaces/~439835659/overview"
    }
  ],
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "license": "Copyright AAIS. All rights reserved",
  "dependencies": {
    "amazon-cognito-identity-js": "^5.2.9",
    "archiver": "^4.0.1",
    "aws-xray-sdk-core": "^3.3.6",
    "ci": "^2.2.0",
    "circleci": "^0.3.3",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "node-fetch": "^2.6.7",
    "remove-bom-stream": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "aws-sdk-mock": "^4.5.0",
    "cypress": "^12.3.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-log-to-output": "^1.1.2",
    "cypress-multi-reporters": "^1.6.1",
    "cypress-promise": "^1.1.0",
    "eslint": "^8.21.0",
    "eslint-plugin-react": "^7.32.1",
    "husky": "^4.2.5",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "node-xlsx": "^0.21.0",
    "prettier": "^2.7.1",
    "read-excel-file": "^5.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm version --force --no-git-tag-version patch && git add package.json package-lock.json"
    }
  }
}
